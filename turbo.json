{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env.local",
    ".env"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.development.local",
        ".env.local",
        ".env.production",
        ".env.development",
        ".env"
      ]
    },
    "start": {
      "persistent": true,
      "dependsOn": [
        "^build",
        "build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.development.local",
        ".env.local",
        ".env.production",
        ".env.development",
        ".env"
      ]
    },
    "@orbiting/backend-modules-auth#build": {
      "cache": false
    },
    "@orbiting/backend-modules-base#build": {
      "cache": false
    },
    "@orbiting/backend-modules-calendar#build": {
      "cache": false
    },
    "@orbiting/backend-modules-call-to-actions#build": {
      "cache": false
    },
    "@orbiting/backend-modules-databroom#build": {
      "cache": false
    },
    "@orbiting/backend-modules-dataloader#build": {
      "cache": false
    },
    "@orbiting/backend-modules-discussions#build": {
      "cache": false
    },
    "@orbiting/backend-modules-invoices#build": {
      "cache": false
    },
    "@orbiting/backend-modules-publikator#build": {
      "cache": false
    },
    "@orbiting/backend-modules-republik#build": {
      "cache": false
    },
    "@orbiting/backend-modules-republik-crowdfundings#build": {
      "cache": false
    },
    "@orbiting/backend-modules-utils#build": {
      "cache": false
    },
    "@project-r/styleguide#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "env": [
        "SERVER"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build",
        "build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.test.local",
        ".env.local",
        ".env.test",
        ".env"
      ]
    },
    "@orbiting/backend-modules-test#test": {
      "cache": false
    },
    "prepare": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
